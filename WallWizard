from fractions import Fraction as fr

t = int(input())
results = []
for i in range(t):
    n, k = map(int, input().split())
    ratio = fr(n,k)
    denominator = ratio.denominator
    steps = 0

    while denominator % 2 == 0:
        denominator //= 2
        steps += 1
    if denominator > 1:
        results.append(-1)
    else:
        results.append(steps)

print("\n".join(map(str,results)))